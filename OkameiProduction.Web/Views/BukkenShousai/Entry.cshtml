        <div class="main-content-footer mx-auto">
            <button type="button" id="btnKakousiji" class="btn btn-info">加工指示</button>
            <button type="button" id="btnShoudakusho" class="btn btn-info">加工承諾書</button>
            <button type="button" id="btnJissekiNippou" class="btn btn-info">ﾌﾟﾚｶｯﾄ実績日報</button>
            <button type="button" id="btnReturn" class="btn btn-secondary" nofocus>戻る</button>
            <button type="button" id="btnSave" class="btn btn-primary active">登録</button>
        </div>
    </div>
</div>

@section Scripts{
    <script type="text/javascript">
        var btnReturn = $('#btnReturn');
        var btnSave = $('#btnSave');
        var btnSet = $('#btnSet');
        var txtBukkenNO = $('#BukkenNO');
        var txtBukkenName = $('#BukkenName');
        var txtSitenCD = $('#SitenCD');
        var tantouEigyouDDL = $('#TantouEigyouCD');
        var chkNoukiMitei = $('#NoukiMiteiCheck');
        var txtNouki = $('#Nouki');

        //------------------------------
        function addEvents() {
            btnReturn.click(function () {
                showConfirmMessage("Q003", function () {
                    if ('@Model.Mode' == 'New') {
                        location.href = '@Url.Action("Index", "Home")';
                    }
                    else {
                        location.href = '@Html.Raw(ViewBag.PreviousUrl)';
                    }
                });
            });

            btnSave.click(function () {
                if (checkErrorOnSave('#main')) {
                    btnSaveClick();
                }
            });

            btnSet.click(function () {
                getNewBukkenNO();
            });

            txtSitenCD.change(function () {
                var key = $(this).val();
                setDropDownList('#TantouEigyouCD', '@Url.Action("GetTantouEigyouSelectList", "api/BukkenShousaiApi")', key);
                setDropDownList('#KoumutenCD', '@Url.Action("GetKoumutenSelectList", "api/BukkenShousaiApi")', key);
            });

            chkNoukiMitei.change(function () {
                txtNouki.setDisabled($(this).prop('checked'));
            });
        }

        function addValidate() {
            //必須チェック
            setRequired('#SitenCD');
            setRequired('#BukkenName');
            setRequired('#KoumutenCD');
            setRequired('#KakouTubosuu');
            setRequired('#Nouki');
            setRequired('#TantouEigyouCD');
            setRequired('#JuchuuDate');
            //日付チェック
            setDateTypeValidate('#Nouki')
            setDateTypeValidate('#UnsougaishaSoukoiribi')
            setDateTypeValidate('#ZairyouNouki')
            setDateTypeValidate('#JuchuuDate')
            setDateTypeValidate('#HusezuTeishutuDate')
            setDateTypeValidate('#KakouShouninDate')
            setDateTypeValidate('#KidasiDate')
            setDateTypeValidate('#KakouSijishoHakkouDate')
            setDateTypeValidate('#KannouDate')
            setDateTypeValidate('#CancelDate')
            //半角チェック
            //setIsHalfWidthValidate('');
            //全角桁数チェック
            setDoubleByteValidate('#BukkenName', true);
            setDoubleByteValidate('#Address');
            setDoubleByteValidate('#TokuchuuzaiComment');
            //数値チェック
            setNumericValidate('#KakouTubosuu', 3, 2)
            setNumericValidate('#KakouNissuu', 3, 0);
            setNumericValidate('#HagarazaiNumber', 3, 0);
            setNumericValidate('#YukazaiNumber', 3, 0);
            setNumericValidate('#NojizaiNumber', 3, 0);
            setNumericValidate('#TekakouTime', 3, 1);
            setNumericValidate('#HundeggerTime', 3, 1);
        }

        function setScreen(mode) {
            if (mode != 'New') {
                btnSet.hide();
            }

            if (mode == 'Delete') {
                setDisabledAll('#main');
            }
        }

        function getNewBukkenNO() {

            if (txtBukkenNO.val() != "") {
                txtBukkenName.focus();
                return;
            }

            var sitencd = txtSitenCD.val();
            if (sitencd == "") {
                txtSitenCD.focus();
                return;
            }

            var ret = calltoApiController('@Url.Action("GetBukkenNO", "api/BukkenShousaiApi")', sitencd);
            if (!ret) {
                return;
            }
            if (ret && ret.MessageID) {
                showMessage(ret);
                return;
            }
            if (ret && ret.NewBukkenNO) {
                txtBukkenNO.val(ret.NewBukkenNO);
                txtBukkenName.focus();
            }
        }

        function btnSaveClick() {
            if (txtBukkenNO.val() == "") {
                txtSitenCD.focus();
                showMessage('E288');
                return false;
            }

            var model = {
                BukkenNO: txtBukkenNO.val(),
                SitenCD: txtSitenCD.val(),
                BukkenName: txtBukkenName.val(),
                Address: $('#Address').val(),
                KoumutenCD: $('#KoumutenCD').val(),
                KakouTubosuu: $('#KakouTubosuu').val(),
                NoukiMiteiCheck: chkNoukiMitei.prop('checked'),
                Nouki: txtNouki.val(),
                UnsougaishaSoukoiriDate: $('#UnsougaishaSoukoiriDate').val(),
                KubunCD: $('#KubunCD').val(),
                TantouEigyouCD: tantouEigyouDDL.val(),
                PCSupportCD: $('#PCSupportCD').val(),
                TantouCadCD: $('#TantouCadCD').val(),
                NyuuryokusakiCD: $('#NyuuryokusakiCD').val(),
                TokuchuuzaiUmu: $('#TokuchuuzaiUmu').val(),
                ZairyouNouki: $('#ZairyouNouki').val(),
                TokuchuuzaiComment: $('#TokuchuuzaiComment').val(),
                //Page 2.
                JuchuuDate: $('#JuchuuDate').val(),
                HusezuTeishutuDate: $('#HusezuTeishutuDate').val(),
                KakouShouninDate: $('#KakouShouninDate').val(),
                KidasiDate: $('#KidasiDate').val(),
                KakouSijishoHakkouDate: $('#KakouSijishoHakkouDate').val(),
                KannouDate: $('#KannouDate').val(),
                CancelDate: $('#CancelDate').val(),
                KakouNissuu: $('#KakouNissuu').val(),
                UpdateDatetime: $('#UpdateDatetime').val(),
                KanamonoCD: $('#KanamonoCD').val(),
                OukazaiExistence: $('#OukazaiExistence').val(),
                OukazaiCheck: $('#OukazaiCheck').prop('checked'),
                KabeExistence: $('#KabeExistence').val(),
                KabeCheck: $('#KabeCheck').prop('checked'),
                HasirazaiExistence: $('#HasirazaiExistence').val(),
                HasirazaiCheck: $('#HasirazaiCheck').prop('checked'),
                HiuchiExistence: $('#HiuchiExistence').val(),
                HiuchiCheck: $('#HiuchiCheck').prop('checked'),
                HagarazaiExistence: $('#HagarazaiExistence').val(),
                HagarazaiCheck: $('#HagarazaiCheck').prop('checked'),
                HagarazaiNumber: $('#HagarazaiNumber').val(),
                YukaExistence: $('#YukaExistence').val(),
                YukaCheck: $('#YukaCheck').prop('checked'),
                YukaGoubanCD: $('#YukaGoubanCD').val(),
                YukaNumber: $('#YukaNumber').val(),
                NojiExistence: $('#NojiExistence').val(),
                NojiCheck: $('#NojiCheck').prop('checked'),
                NojiGoubanCD: $('#NojiGoubanCD').val(),
                NojiNumber: $('#NojiNumber').val(),
                HundeggerExistence: $('#HundeggerExistence').val(),
                HundeggerCheck: $('#HundeggerCheck').prop('checked'),
                TekakouTime: $('#TekakouTime').val(),
                HundeggerTime: $('#HundeggerTime').val(),
                //Page 3.
                //Page 4.
                Comment: $('#Comment').val(),
            };

        }

        //------------------------------
        $(document).ready(function () {
            addEvents();
            addValidate();
            setScreen('@Model.Mode');

            if ('@Model.Mode' == 'Delete') {
                btnSave.focus();
            }
            else {
                txtSitenCD.focus();
            }
        });

    </script>
}
